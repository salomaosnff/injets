{
  "name": "@injets/core",
  "version": "1.0.3",
  "description": "A lightweight dependency injection container for JavaScript/TypeScript",
  "repository": "https://github.com/salomaosnff/injets",
  "module": "./dist/index.mjs",
  "author": "Salom√£o Neto <salomaosnff@yahoo.com>",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "example": "tsx ./example/index.ts",
    "build": "unbuild",
    "prepublish": "pnpm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@injets/core": "^1.0.0"
  },
  "devDependencies": {
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0"
  }
}
